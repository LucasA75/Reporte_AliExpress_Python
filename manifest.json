{
  "manifest_version": 3,
  "name": "Aliexpress order to Excel",
  "description": "Get your latest orders to an Excel sheet",
  "version": "1",
  "icons": {
    "128": "./assets/icon_128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["infoOrder.js"],
      "run_at": "document_idle"
    }
  ],
  "module": {
    "type": "module"
  },
  "host_permissions": [
    "https://www.google.com/*"
  ]
}
